<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="waterTemperature" type="ChipAquariumMobileService.Config.WaterTemperatureSettings, ChipAquariumMobileServiceTest"/>
  </configSections>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.1"/>
    </startup>
  <waterTemperature>
    <normalRange from="23.0" to="28.0"/>
    <alert renotifyElapsedTime="24:00:00" notifyMailAddress="main@takeauk.com"/>
  </waterTemperature>
</configuration>
